strokeLinejoin: \"round\",\n    strokeWidth: 1.2538\n  })), _g4 || (_g4 = /*#__PURE__*/React.createElement(\"g\", null, /*#__PURE__*/React.createElement(\"path\", {\n    d: \"m122.52 62.395h2.273v8.3906l8.391-8.3906h3.222l-7.16 6.9375 7.36 10.277h-3.036l-6.023-8.6367-2.754 2.6367v6h-2.273v-17.215z\",\n    clipPath: \"url(#d)\"\n  }), /*#__PURE__*/React.createElement(\"path\", {\n    d: \"m145.97 61.949c1.617 0 3.015 0.3125 4.195 0.9375 1.711 0.8984 2.758 2.4726 3.141 4.7226h-2.309c-0.281-1.2578-0.863-2.1738-1.746-2.748s-1.996-0.8613-3.34-0.8613c-1.594 0-2.935 0.5976-4.025 1.7929-1.09 1.1954-1.635 2.9766-1.635 5.3438 0 2.0469 0.449 3.7129 1.348 4.998 0.898 1.2852 2.363 1.9278 4.394 1.9278 1.555 0 2.842-0.4512 3.862-1.3535 1.019-0.9024 1.541-2.3614 1.564-4.377h-5.391v-1.9336h7.559v9.2109h-1.5l-0.563-2.2148c-0.789 0.8672-1.488 1.4688-2.097 1.8047-1.024 0.5781-2.324 0.8672-3.903 0.8672-2.039 0-3.792-0.6602-5.261-1.9805-1.602-1.6562-2.403-3.9297-2.403-6.8203 0-2.8828 0.782-5.1758 2.344-6.8789 1.485-1.625 3.406-2.4375 5.766-2.4375z\",\n    clipPath: \"url(#d)\"\n  }))));\n}\nconst ForwardRef = /*#__PURE__*/React.forwardRef(SvgSmallProgressImg);\nexport default __webpack_public_path__ + \"static/media/SmallProgressImg.6b3c7b50.svg\";\nexport { ForwardRef as ReactComponent };","import {Badge, ListGroup} from \"react-bootstrap\";\nimport Container from \"react-bootstrap/Container\";\nimport {ReactComponent as ShowSmallProgressImg} from \"../../img/SmallProgressImg.svg\";\nimport React from \"react\";\n\n/*\n Componente che visualizza i dettagli di un singolo progresso (es. lettura peso giornaliero) in un ListGroup\n */\nconst ProgressPreview = ({timestamp, peso, imc, dieta}) => {\n    const data = timestamp.toDate().toLocaleDateString(\"it-IT\");\n    const danger_weight_msg = `Il peso registrato supera di ${peso - dieta.obiettivo_peso} Kg l'obiettivo peso prefissato`;\n    const success_weight_msg = `Il peso registrato è di ${dieta.obiettivo_peso - peso} Kg al di sotto dell'obiettivo peso prefissato`;\n    return (\n        <ListGroup.Item className={\"col-12\"}\n                        style={{padding: 0}}>\n            <Container className={\"d-flex flex-column mx-auto my-auto \"}>\n                <Container className={\"d-flex flex-wrap text-start\"}\n                           style={{padding: 0}}>\n                    <Container className={\"d-flex flex-row justify-content-evenly my-auto gap-1\"}\n                               style={{padding: 0, marginTop: 20}}>\n                        <Container style={{margin: 0}}\n                                   className={\"col-6 col-sm-2 mx-auto my-auto text-center\"}>\n                            <ShowSmallProgressImg\n                                style={{maxHeight: 50, margin: 10}}\n                            />\n                        </Container>\n                        <Container className={\"d-flex flex-wrap align-items-center col-7 col-sm-9\"}\n                                   style={{padding: 0}}>\n                            <Container style={{padding: 0}}\n                                       className={\"col-12 col-sm-3 mb-2 mb-sm-0 mt-2 mt-lg-0\"}>\n                                <div className={\"float-start\"}\n                                     style={{margin: 0}}>Data: <strong>{data}</strong></div>\n                            </Container>\n                            <Container style={{padding: 0, margin: 0}}\n                                       className={\"col-12 col-sm-3 my-auto\"}>\n                                <div style={{margin: 0}}>Peso: <strong>\n                                    {peso > dieta.obiettivo_peso\n                                        ?\n                                        (<div className={\"d-inline-block\"}><Badge pill\n                                                                                  bg=\"danger\"\n                                                                                  style={{marginLeft: 5}}\n                                                                                  title={danger_weight_msg}><p style={{margin: 1}}>{peso} KG</p></Badge>\n                                        </div>)\n                                        :\n                                        (<div className={\"d-inline-block\"}><Badge pill\n                                                                                  bg=\"success\"\n                                                                                  style={{marginLeft: 5}}\n                                                                                  title={success_weight_msg}><p style={{margin: 1}}>{peso} KG</p></Badge>\n                                        </div>)\n                                    }</strong></div>\n                            </Container>\n                            <Container style={{padding: 0}}\n                                       className={\"col-12 col-sm-3\"}>\n                                <div style={{margin: 0}}>IMC: <strong>{imc}</strong></div>\n                            </Container>\n                            <Container style={{padding: 0}}\n                                       className={\"col-12 col-sm-3 mb-2 mb-sm-3 mb-lg-0\"}>\n                                <div style={{margin: 0}}>Obiettivo: <strong>{dieta.obiettivo_peso} KG</strong></div>\n                            </Container>\n                        </Container>\n                    </Container>\n                </Container>\n            </Container>\n        </ListGroup.Item>\n    );\n};\n\nexport default ProgressPreview;","import React, {useEffect, useState} from \"react\";\nimport {Alert, Button, Card, Form, InputGroup, ListGroup, Nav, Navbar} from \"react-bootstrap\";\nimport {useNavigate, useParams} from \"react-router-dom\";\nimport Container from \"react-bootstrap/Container\";\nimport ProgressiImg from \"../../img/ProgressiImg.svg\";\nimport {collection, doc, getDoc, getDocs, orderBy, query} from \"firebase/firestore\";\nimport {db} from \"../../config/firebase-config\";\nimport {SyncLoader} from \"react-spinners\";\nimport Spinner from \"../Utilities/Spinner\";\nimport Pagination from \"../Utilities/Pagination\";\nimport ProgressPreview from \"./ProgressPreview\";\nimport {scrollToTop} from \"../helpers/scroll\";\nimport {LinkContainer} from \"react-router-bootstrap\";\nimport {FontAwesomeIcon} from \"@fortawesome/react-fontawesome\";\nimport {faAngleLeft, faBookOpen} from \"@fortawesome/free-solid-svg-icons\";\n\n/*\n Funzione che si occupa di mostrare un elenco contenente tutte i progressi (es. peso giornaliero) registrati dal paziente\n Gli elementi vengono impaginati in modo tale che ce ne siano 7 per pagina\n */\nexport default function ShowProgress() {\n    //Ottengo l'id dall'url\n    const {id} = useParams();\n    const [error, setError] = useState(\"\");\n    const [warning, setWarning] = useState(\"\");\n    const [diet, setDiet] = useState();\n    const [progress, setProgress] = useState(true);\n    //Numero di progressi visualizzate per pagina\n    const progressPerPage = 7;\n    const [currentPage, setCurrentPage] = useState(1);\n    const [searchInput, setSearchInput] = useState(\"\");\n    const lastProgressNumber = currentPage * progressPerPage;\n    const firstProgressIndex = lastProgressNumber - progressPerPage;\n    const [allProgress, setAllProgress] = useState([]);\n    const [progressi_titolo, setProgressi_titolo] = useState(\"\");\n    const navigate = useNavigate();\n\n    useEffect(() => {\n        //Ad ogni caricamento la pagina effettua uno scroll fino al suo inizio\n        scrollToTop();\n        const getAllProgress = async () => {\n            setProgress(true);\n            //Controllo l'esistenza della dieta\n            const docRef = doc(db, \"diets\", id);\n            await getDoc(docRef)\n                .then(async (doc) => {\n                    if (doc.exists()) {\n                        await getDocs(query(collection(db, \"diets\", id, \"pesate\"),\n                            orderBy(\"timestamp\", \"desc\")))\n                            .then((querySnapshot) => {\n                                if (querySnapshot.empty) {\n                                    setWarning(\"Non è stato ancora utilizzato il bot di telegram 'YourFoodCoachBot' per registrare progressi\");\n                                } else {\n                                    setAllProgress(querySnapshot.docs.map((doc) => ({...doc.data(), id: doc.id})));\n                                    setCurrentPage(1);\n                                }\n                            }).catch((error) => setError(\"Si è verificato un errore inaspettato\"));\n                    } else {\n                        //Il paziente richiesto non esiste\n                        //L'utente viene indirizzato alla pagina che visualizza l'elenco dei pazienti con una dieta attiva\n                        navigate(\"/progressi-pazienti\", {replace: true});\n                    }\n                }).finally(() => setProgress(false));\n        };\n        getAllProgress();\n        // eslint-disable-next-line react-hooks/exhaustive-deps\n    }, []);\n\n    useEffect(() => {\n        const getDiet = async () => {\n            setProgress(true);\n            const docRef = doc(db, \"diets\", id);\n            await getDoc(docRef)\n                .then((doc) => {\n                    if (doc.exists()) {\n                        setDiet(doc.data());\n                        if (doc.data().tipologia_dieta === \"Nutrizionista\") {\n                            setProgressi_titolo(\"Progressi Paziente\");\n                        } else if (doc.data().tipologia_dieta === \"Utente\") {\n                            setProgressi_titolo(\"I tuoi Progressi\");\n                        }\n                        setProgress(false);\n                    }\n                });\n        };\n        getDiet();\n        // eslint-disable-next-line react-hooks/exhaustive-deps\n    }, []);\n\n    const filtered = allProgress.filter(\n        //Funzione di ricerca: cerca la stringa data in input in timestamp\n        (progress) => {\n            return searchInput.toLowerCase() === \"\" ? progress\n                : progress.timestamp.toDate().toLocaleDateString(\"it-IT\").includes(searchInput.toLowerCase());\n        }\n    );\n\n    if (progress === true) {\n        return <Spinner/>;\n    }\n\n\n    return (\n        <>\n            {progress === false && <Container className={\"modal-fullscreen\"}\n                                              style={{\n                                                  background: \"linear-gradient(123deg, rgba(247,209,98,1) 30%, rgba(251,149,96,1) 50%, rgba(254,88,94,1) 70%)\",\n                                                  backgroundSize: \"cover\",\n                                                  minHeight: \"inherit\",\n                                                  padding: 0\n                                              }}>\n                <Container className={\"modal-fullscreen text-center\"}>\n                    <Container className={\"d-flex flex-column mx-auto my-auto\"}\n                               style={{minHeight: 450, marginTop: 20}}>\n                        <Card className={\"col-12 col-sm-12 col-md-10 mx-auto\"}\n                              style={{\n                                  marginTop: 20,\n                                  marginBottom: 20,\n                                  borderRadius: 16,\n                                  boxShadow: \"10px 10px 10px rgba(30,30,30,0.5)\",\n                                  borderLeft: \"solid 1px rgba(255,255,255,0.8)\",\n                                  borderTop: \"solid 1px rgba(255,255,255,0.8)\"\n                              }}>\n                            <h2 style={{fontFamily: \"Arial\", marginTop: 10}}>\n                                <div className={\"d-flex my-auto col-12\"}>\n                                    <div className={\"col-12\"}>\n                                        <div className={\"float-start\"}\n                                             style={{marginTop: 10, marginLeft: 20}}>\n                                            <LinkContainer to=\"/progressi-pazienti\"\n                                                           className={\"my-auto\"}>\n                                                <Button style={{height: 40, width: 100}}\n                                                        variant={\"warning\"}\n                                                ><FontAwesomeIcon icon={faAngleLeft}\n                                                                  color={\"black\"}/></Button>\n                                            </LinkContainer>\n                                        </div>\n                                    </div>\n                                </div>\n                                <div className={\"d-flex d-inline-block col-12 justify-content-center\"}>\n                                    <img src={ProgressiImg}\n                                         className={\"col-12 col-sm-3\"}\n                                         style={{maxHeight: 90, maxWidth: 90, marginRight: 20}}\n                                         alt={\"Bilancia\"}/>\n                                    <h5 className={\"my-auto\"}>{progressi_titolo}: <p>\n                                        <strong>{diet.cognome_paziente} {diet.nome_paziente}</strong></p></h5>\n                                </div>\n                            </h2>\n                            <SyncLoader color=\"#eb6164\"\n                                        loading={progress}\n                                        style={{\n                                            position: \"absolute\",\n                                            top: \"50%\",\n                                            left: \"50%\",\n                                            transform: \"translate(-50%, -50%)\",\n                                            zIndex: 1\n                                        }}/>\n                            <Form className={\"d-flex flex-column\"}>\n                                <Container className={\"d-flex flex-wrap justify-content-evenly\"}\n                                           style={{marginBottom: 20, marginTop: 10, minHeight: 500}}>\n                                    <ListGroup className={\"col-12 mx-auto\"}\n                                               style={{\n                                                   marginBottom: 20\n                                               }}>\n                                        <Navbar expand=\"lg\"\n                                                className={\"justify-content-end\"}\n                                                style={{marginBottom: 10}}>\n                                            <Container className={\"col-5 col-lg-4 d-flex my-auto flex-wrap\"}\n                                                       style={{marginBottom: 10, padding: 0}}>\n                                                <LinkContainer to= {`/diario-alimentare/${id}`}\n                                                               className={\"my-auto\"}\n                                                               style={{marginRight: 20}}>\n                                                    <Nav.Link>\n                                                        <Button variant=\"outline-secondary\"\n                                                                className={\"flex-wrap mx-auto\"}\n                                                                onClick={() => navigate(`/diario-alimentare/${id}`, {replace: true})}><FontAwesomeIcon icon={faBookOpen}\n                                                                                                                                           color={\"secondary\"} style={{marginRight: 10}}/>Diario Alimentare </Button>\n                                                    </Nav.Link>\n                                                </LinkContainer>\n                                            </Container>\n                                            <Container className={\"col-6 col-sm-6 col-lg-5\"}\n                                                       style={{margin: 0, padding: 0}}>\n                                                <InputGroup className=\"d-flex \">\n                                                    <Form.Control\n                                                        type=\"search\"\n                                                        placeholder={\"Cerca data...\"}\n                                                        className={\"me-2\"}\n                                                        aria-label=\"Search\"\n                                                        value={searchInput}\n                                                        onChange={(e) => {\n                                                            setCurrentPage(1);\n                                                            setSearchInput(e.target.value);\n                                                        }}\n                                                    />\n                                                </InputGroup>\n                                            </Container>\n                                        </Navbar>\n                                        {filtered.slice(firstProgressIndex, lastProgressNumber)\n                                                 .map((progress) =>\n                                                     <ProgressPreview key={progress.id}\n                                                                      timestamp={progress.timestamp}\n                                                                      peso={progress.peso}\n                                                                      imc={progress.imc}\n                                                                      dieta={diet}\n                                                     />)}\n                                        {warning && <Alert className={\"mx-auto col-10 flex-wrap\"}\n                                                           variant={\"warning\"}\n                                                           style={{margin: 30}}>{warning}</Alert>}\n                                        {filtered.length === 0 && !error && allProgress.length > 0\n                                            ? (<Alert variant={\"warning\"}\n                                                      style={{margin: 30}}>Nessun dato soddisfa i\n                                                                           parametri di ricerca</Alert>)\n                                            : (<></>)\n                                        }\n                                    </ListGroup>\n                                    <Container className={\"d-flex flex-wrap justify-content-center mt-auto\"}>\n                                        {allProgress.length !== 0 && <Pagination\n                                            itemsCount={filtered.length}\n                                            itemsPerPage={progressPerPage}\n                                            currentPage={currentPage}\n                                            setCurrentPage={setCurrentPage}\n                                            alwaysShown={true}\n                                        />\n                                        }\n                                    </Container>\n                                </Container>\n                            </Form>\n                        </Card>\n                    </Container>\n                </Container>\n            </Container>\n            }\n        </>\n    );\n}\n"]}}